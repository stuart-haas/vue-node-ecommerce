{
  "name": "vuex-server",
  "version": "1.0.0",
  "description": "vuex server",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "author": "Artisan Digital Media",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bulma": "^0.8.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dropzone": "^5.5.1",
    "exifr": "^2.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.0",
    "helmet": "^3.21.2",
    "multer": "^1.4.2",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.1",
    "pg": "^7.14.0",
    "pug": "^2.0.4",
    "request": "^2.88.0",
    "typeorm": "^0.2.21",
    "typeorm-store": "^1.2.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3",
    "@types/chai": "^4.2.7",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.17",
    "@types/pg": "^7.11.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "reflect-metadata": "^0.1.13",
    "reload": "^3.0.3",
    "supertest": "^4.0.2"
  }
}
